{
  "name": "gatsby-plugin-stylus-resources",
  "description": "Gatsby support for Stylus with resources (e.g. variables, mixins etc.)",
  "version": "1.0.35",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-stylus",
    "stylus",
    "stylus-resources",
    "sass-resources"
  ],
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint src --fix",
    "pretty": "prettier --write 'src/**/*.js'",
    "test": "echo 'No test specified'",
    "clean": "rimraf ./gatsby-node.js",
    "build": "babel src --out-dir .",
    "prepare": "yarn clean && cross-env NODE_ENV=production yarn build",
    "watch": "babel -w src --out-dir .",
    "semantic-release": "semantic-release"
  },
  "author": "Jimmy Beldone <dev.jimmy.beldone@gmail.com>",
  "bugs": {
    "url": "https://github.com/JimmyBeldone/gatsby-plugin-stylus-resources/issues"
  },
  "license": "MIT",
  "main": "./index.js",
  "peerDependencies": {
    "gatsby": "^3.0.0"
  },
  "homepage": "https://github.com/JimmyBeldone/gatsby-plugin-stylus-resources",
  "repository": {
    "type": "git",
    "url": "https://github.com/JimmyBeldone/gatsby-plugin-stylus-resources.git"
  },
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "sass-resources-loader": "^2.0.3",
    "stylus": "^0.54.8",
    "stylus-loader": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.13.10",
    "@babel/core": "7.13.10",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "babel-eslint": "10.1.0",
    "babel-preset-gatsby-package": "1.0.0",
    "commitizen": "4.2.3",
    "cross-env": "7.0.3",
    "devmoji": "^2.2.0",
    "eslint": "7.21.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-prettier": "3.3.1",
    "husky": "4.3.8",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "semantic-release": "17.4.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "prepare-commit-msg": "npx --no-install devmoji -e --lint"
    }
  },
  "lint-staged": {
    "{src}/**/*.js": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
