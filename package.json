{
  "name": "gatsby-plugin-stylus-resources",
  "description": "Gatsby support for Stylus with resources (e.g. variables, mixins etc.)",
  "version": "1.0.22",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "gatsby-plugin-stylus",
    "stylus",
    "stylus-resources",
    "sass-resources"
  ],
  "scripts": {
    "commit": "git-cz",
    "lint": "eslint src --fix",
    "pretty": "prettier --write 'src/**/*.js'",
    "test": "echo 'No test specified'",
    "clean": "rimraf ./gatsby-node.js",
    "build": "babel src --out-dir .",
    "prepare": "yarn clean && cross-env NODE_ENV=production yarn build",
    "watch": "babel -w src --out-dir .",
    "semantic-release": "semantic-release",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect"
  },
  "author": "Jimmy Beldone <dev.jimmy.beldone@gmail.com>",
  "bugs": {
    "url": "https://github.com/JimmyBeldone/gatsby-plugin-stylus-resources/issues"
  },
  "license": "MIT",
  "main": "./index.js",
  "peerDependencies": {
    "gatsby": "^2.0.0"
  },
  "homepage": "https://github.com/JimmyBeldone/gatsby-plugin-stylus-resources",
  "repository": {
    "type": "git",
    "url": "https://github.com/JimmyBeldone/gatsby-plugin-stylus-resources.git"
  },
  "dependencies": {
    "@babel/runtime": "^7.4.5",
    "sass-resources-loader": "^2.0.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "snyk": "^1.192.3"
  },
  "devDependencies": {
    "@babel/cli": "7.5.0",
    "@babel/core": "7.5.4",
    "@semantic-release/changelog": "3.0.4",
    "@semantic-release/git": "7.0.16",
    "babel-eslint": "10.0.2",
    "babel-preset-gatsby-package": "0.2.2",
    "commitizen": "3.1.2",
    "cross-env": "5.2.0",
    "eslint": "6.0.1",
    "eslint-config-airbnb-base": "13.2.0",
    "eslint-config-prettier": "6.0.0",
    "eslint-plugin-import": "2.18.0",
    "eslint-plugin-prettier": "3.1.0",
    "husky": "3.0.0",
    "lint-staged": "9.2.0",
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.18"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src}/**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "snyk": true
}
